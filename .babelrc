{
    "presets": [
        "@babel/preset-env",
        ["@babel/preset-react", {
            "pragma": "h",
            "pragmaFrag": "Fragment"
        }],
        "@babel/preset-typescript"
    ],
    "plugins": [
        ["@babel/plugin-transform-react-jsx", {
            "pragma": "h",
            "pragmaFrag": "Fragment"
        }],
        "@babel/plugin-proposal-class-properties"
    ]
}
